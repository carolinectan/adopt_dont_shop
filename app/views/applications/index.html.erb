<h1>All Applications</h1>
<br>

<% @applications.each do |application| %>
  <h3><%= application.name %></h3>
  <p><%= application.street %></p>
  <p><%= application.city %></p>
  <p><%= application.state %></p>
  <p><%= application.zip_code %></p>
  <p><%= application.application_status %></p>
  <br>
<% end %>
